<div class="page-header">
  <%= link_to ad_type_fields_path, class: 'btn btn-default' do %>
    All Ad_type_fields
  <% end %>
  <%= link_to edit_ad_type_field_path(@ad_type_field), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show ad_type_field</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @ad_type_field.name %></dd>
  <dt>Add type:</dt>
  <dd><%= @ad_type_field.ad_type_id %></dd>
  <dt>Form field type:</dt>
  <dd><%= @ad_type_field.form_field_type %></dd>
  <dt>Help text:</dt>
  <dd><%= @ad_type_field.help_text %></dd>
  <dt>Text before:</dt>
  <dd><%= @ad_type_field.text_before %></dd>
  <dt>Text after:</dt>
  <dd><%= @ad_type_field.text_after %></dd>
  <dt>Required:</dt>
  <dd><%= @ad_type_field.required %></dd>
  <dt>Position:</dt>
  <dd><%= @ad_type_field.position %></dd>
</dl>
